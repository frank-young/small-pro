<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">匹配对象</view>
            <view class="weui-msg__title">匹配度：{{100-matchInfo.offset}}%</view>
            <view class="weui-msg__desc">姓名：{{matchInfo.name}}</view>
            <view class="weui-msg__desc">生日：{{matchInfo.birthday}}</view>
            <view class="weui-msg__desc">城市：{{matchInfo.province}}-{{matchInfo.city}}</view>
            <view class="weui-msg__desc">学校：{{matchInfo.school}}</view>
            <view class="weui-msg__desc">房间号：{{matchInfo.room_num}}</view>
            <view class="weui-msg__desc">个人匹配号：{{matchInfo.self_num}}</view>
        </view>
    </view>
</view>
